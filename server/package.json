{
  "name": "prometheus-community-server",
  "version": "1.0.0",
  "description": "Backend API for Prometheus Community Dashboard",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watchAll",
    "build": "babel src -d dist",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.8.1",
    "compression": "^1.7.4",
    "pg": "^8.11.1",
    "knex": "^2.5.1",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.38.0",
    "nodemailer": "^6.9.4",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "Prometheus Team <hello@prometheuscommunity.com>",
  "license": "MIT"
}
